{
  "name": "my-php-framework",
  "description": "A simple PHP framework for building web applications.",
  "type": "project",
  "require": {
    "php": "^7.4 || ^8.0",
    "monolog/monolog": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Src\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}